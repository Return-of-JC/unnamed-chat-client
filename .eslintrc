{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "solid", 
        "prettier",
    ],
    "extends": [
        "eslint:recommended", 
        "plugin:solid/typescript", 
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"] 
    },
    "rules": {
        "no-unused-vars": "warn",
        "prettier/prettier": ["error"]
    }
}
